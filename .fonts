<?xml version='1.0'?>
<!DOCTYPE fontconfig SYSTEM 'fonts.dtd'>
<fontconfig>

 <alias>
  <family>sans-serif</family>
  <prefer>
   <family>Cabin</family>
  </prefer>
 </alias>

 <alias>
  <family>serif</family>
  <prefer>
   <family>Cabin</family>
  </prefer>
 </alias>

 <alias>
  <family>monospace</family>
  <prefer>
   <family>Cabin</family>
  </prefer>
 </alias>
 
 <alias>
  <family>mono</family>
  <prefer>
   <family>Noto Sans Mono</family>
  </prefer>
 </alias>

</fontconfig>
