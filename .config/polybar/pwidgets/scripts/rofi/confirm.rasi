/* Confirm Dialog */

@import "colors.rasi"

* {
    background-color:       @bg;
    text-color:             @fg;
	font:				 	"Iosevka Nerd Font 10";
}

window {
    width:      			230px;
    padding:    			20px;
    border:					0px;
    border-radius:          20px;
    border-color:           @ac;
    location:               southwest;
    x-offset:               25px;
    y-offset:               -95px;
}

entry {
    expand: 				true;
    text-color:				@ac;
}
